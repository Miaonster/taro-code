{
  "name": "taro-code",
  "version": "4.0.0-beta.0",
  "description": "Taro.js barcode & qrcode",
  "main": "lib/index.js",
  "source": "src/index.ts",
  "exports": {
    ".": "./lib/index.js",
    "./Barcode": {
      "default": "./lib/components/Barcode/index.js",
      "import": "./lib/components/Barcode/index.js",
      "types": "./lib/components/Barcode/index.d.ts"
    },
    "./QRCode": {
      "default": "./lib/components/QRCode/index.js",
      "import": "./lib/components/QRCode/index.js",
      "types": "./lib/components/QRCode/index.d.ts"
    },
    "./QRCodeCanvas": {
      "default": "./lib/components/QRCodeCanvas/index.js",
      "import": "./lib/components/QRCodeCanvas/index.js",
      "types": "./lib/components/QRCodeCanvas/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsc --project ./tsconfig.build.json --watch --declaration --incremental",
    "build": "tsc --project ./tsconfig.build.json --declaration",
    "prepublishOnly": "yarn run clean && yarn run build",
    "lint": "eslint ./src --fix",
    "lint:tsc": "tsc --noEmit  --skipLibCheck",
    "lint:style": "stylelint \"src/**/*.scss\" --syntax scss",
    "lint:style-fix": "stylelint \"src/**/*.scss\" --syntax scss --fix",
    "test": "cross-env NODE_ENV=test && jest --coverage",
    "test:ci": "yarn run build:h5 && yarn run test",
    "test:analyze": "rollup --config ./config/rollup.config.analyze.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "rimraf .temp dist lib coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miaonster/taro-code.git"
  },
  "keywords": [
    "taro",
    "qrcode",
    "barcode"
  ],
  "files": [
    "lib",
    "dist",
    "types"
  ],
  "author": "miaonster",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/miaonster/taro-code/issues"
  },
  "homepage": "https://github.com/miaonster/taro-code#readme",
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@tarojs/cli": "3.6.2",
    "@tarojs/components": "3.6.2",
    "@tarojs/mini-runner": "3.6.2",
    "@tarojs/react": "3.6.2",
    "@tarojs/runtime": "3.6.2",
    "@tarojs/taro": "3.6.2",
    "@tarojs/webpack-runner": "3.6.2",
    "@testing-library/react": "^11.2.7",
    "@types/classnames": "^2.3.1",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-redux": "^7.1.16",
    "@types/react-test-renderer": "^18.0.0",
    "@types/remote-redux-devtools": "^0.5.4",
    "@types/webpack-env": "^1.13.6",
    "@types/wechat-miniprogram": "^3.3.0",
    "babel-jest": "^27.0.2",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-preset-taro": "3.6.2",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.2",
    "jest": "^26.4.1",
    "nervjs": "^1.5.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.4",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "rollup-plugin-visualizer": "^4.1.0",
    "taro-testing-library": "^1.1.0",
    "ts-jest": "^27.0.3",
    "typescript": "4.3.2"
  },
  "peerDependencies": {
    "@tarojs/components": ">=3",
    "@tarojs/mini-runner": ">=3",
    "@tarojs/react": ">=3",
    "@tarojs/runtime": ">=3",
    "@tarojs/taro": ">=3",
    "@tarojs/webpack-runner": ">=3",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}
